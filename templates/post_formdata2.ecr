<!DOCTYPE html>
<html lang="ja">
<head>
 <meta charset="utf-8" />
 <meta name="viewport" content="width=device-width,initial-scale=1" />
 <title>post_formdata2</title>
 <!-- BootstrapのCSS読み込み -->
 <link href="https://cdn.jsdelivr.net/npm/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
 <style>
 .bgcolor {
      background-color: whitesmoke;
      border-radius: 8px;
      padding: 10px
    }
 </style>
 <script>
   async function button_click() {
       const formdata = new FormData(form1);
       const response = await fetch("/post_formdata2", {method:"POST", headers:{"Content-Type":"multipart/form-data"}, body:formdata});
       const data = await response.json();
       const result = document.getElementById("result");
       const json = JSON.stringify(data);
       result.innerText = json
   }
 </script>
</head>
 
<body>
 <!-- ヘッダー -->
 <header class="container">
  <h1 class="header-1 text-center p-5 bgcolor">post_formdata2 (multipart)</h1>
  <p class="text-center"><a href="/">HOME</a></p>
 </header>

 <!-- 本文 -->
 <article class="container">
   <form name="form1" class="row" method="POST" enctype="multipart/form-data" action="/post_formdata2">
    <div class="mt-3"><label class="form-control-label">X<input class="form-control" type="text" name="X" value="0" /></label></div>
    <div class="mt-3"><label class="form-control-label">Y<input class="form-control" type="text" name="Y" value="0" /></label></div>
    <div class="mt-3"><label class="form-control-label">file1<input class="form-control" type="file" name="file1" /></label></div>
    <div class="mt-5"><button type="button" class="btn btn-secondary" onclick="button_click()">送信する</button></div>
   </form>
   <div class="row mt-5" id="result"></div>
 </article>
 
 <!-- フッター -->
 <footer class="container">
  <p class="text-center mt-4"><a href="#top">TOP</a></p>
  <p>&nbsp;</p>
 </footer>
 <!-- BootstrapのJS読み込み -->
 <script src="https://cdn.jsdelivr.net/npm/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
